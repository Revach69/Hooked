        "**/node_modules/**/gradle": true,
        "**/node_modules/**/gradlew": true,
        "**/node_modules/**/gradlew.bat": true,
        "**/node_modules/**/gradle.properties": true,
        "**/node_modules/**/settings.gradle": true,
        "**/node_modules/**/settings.gradle.kts": true,
        "**/node_modules/**/src/main/java": true,
        "**/node_modules/**/src/main/kotlin": true,
        "**/node_modules/**/src/test/java": true,
        "**/node_modules/**/src/test/kotlin": true,
        "**/node_modules/**/expo-gradle-plugin": true,
