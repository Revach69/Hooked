{
  "expo": {
    "name": "Hooked",
    "slug": "hooked",
    "version": "1.0.1",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "backgroundColor": "#ffffff",
      "dark": {
        "backgroundColor": "#1a1a1a"
      }
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hookedapp.app",
      "googleServicesFile": "./GoogleService-Info.plist",
      "associatedDomains": [
        "applinks:www.hooked-app.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Hooked needs camera access to let you take profile photos for your dating profile at events.",
        "NSPhotoLibraryUsageDescription": "Hooked needs photo library access to let you select existing photos for your dating profile at events.",
        "NSUserNotificationUsageDescription": "Hooked needs notification access to send you alerts about new matches and messages at events.",
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "production"
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.hookedapp.app",
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.READ_EXTERNAL_STORAGE",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.WRITE_EXTERNAL_STORAGE",
        "android.permission.VIBRATE",
        "android.permission.INTERNET",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.WAKE_LOCK",
        "android.permission.RECEIVE_BOOT_COMPLETED"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "@sentry/react-native/expo",
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "New Message",
          "androidChannelId": "messages",
          "androidChannelName": "Messages",
          "androidChannelDescription": "Notifications for new messages and matches",
          "androidChannelImportance": "high",
          "androidShowBadge": true,
          "androidVibrate": true,
          "androidSound": "default"
        }
      ],
      [
        "@react-native-firebase/app",
        {
          "android": {
            "googleServicesFile": "./google-services.json"
          },
          "ios": {
            "googleServicesPlist": "./GoogleService-Info.plist"
          }
        }
      ], 
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "useModularHeaders": true,
            "newArchEnabled": false
          }
        }
      ]
    ],
    "scheme": "hooked", 
    "extra": {
      "firebaseConfig": {
