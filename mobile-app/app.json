{
  "expo": {
    "name": "Hooked Dev",
    "slug": "hooked",
    "version": "1.1.8",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-rounded.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*",
      "assets/**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hookedapp.app",
      "buildNumber": "1.1.8",
      "splash": {
        "image": "./assets/icon-rounded.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/icon-rounded.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      },
      "associatedDomains": [
        "applinks:dev.hooked-app.com",
        "applinks:dev-www.hooked-app.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Hooked needs camera access to let you take profile photos and scan QR codes at events.",
        "NSPhotoLibraryUsageDescription": "Hooked needs photo library access to let you select existing photos for your dating profile at events.",
        "NSUserNotificationUsageDescription": "Hooked needs notification access to send you alerts about new matches and messages at events.",
        "UIBackgroundModes": [
          "remote-notification",
          "remote-notification"
        ]
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "googleServicesFile": "./ios/Hooked/GoogleService-Info-dev.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/icon-rounded.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/icon-rounded.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      },
      "package": "com.hookedapp.app",
      "versionCode": 8,
      "googleServicesFile": "./android/app/google-services.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ],
          "data": [
            {
              "scheme": "https",
              "host": "dev.hooked-app.com"
            },
            {
              "scheme": "https",
              "host": "www.hooked-app.com"
            }
          ]
        }
      ],
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_BACKGROUND_LOCATION"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "hooked",
          "project": "hooked-dev",
          "url": "https://sentry.io/"
        }
      ],
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "New Message [DEV]",
          "androidChannelId": "default-dev",
          "androidChannelName": "Hooked Dev Notifications",
          "androidChannelDescription": "Development notifications for new messages and matches",
          "androidChannelImportance": "high",
          "androidShowBadge": true,
          "androidVibrate": true,
          "androidSound": "default"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "useModularHeaders": false,
            "newArchEnabled": false
          },
          "android": {
            "minSdkVersion": 21
          }
        }
      ]
    ],
    "scheme": "hooked",
    "extra": {
      "firebaseConfig": {
        "apiKey": "AIzaSyBhGESZT7wVnFX7zLm6_ztJJHzKL5JLhf8",
        "authDomain": "hooked-development.firebaseapp.com",
        "projectId": "hooked-development",
        "storageBucket": "hooked-development.firebasestorage.app",
        "messagingSenderId": "474843121153",
        "appId": "1:474843121153:web:7242ff23e23f158383a200",
      },
      "mapboxAccessToken": "$MAPBOX_ACCESS_TOKEN",
      "mapboxCacheConfig": {
        "maxCacheSize": 100,
        "cleanupThreshold": 80,
        "maxAge": 7,
        "preferredCachePolicy": "cacheFirst"
      },
      "router": {},
      "eas": {
        "projectId": "7a1de260-e3cb-4cbb-863c-1557213d69f0"
      },
      "updates": {
        "url": "https://u.expo.dev/7a1de260-e3cb-4cbb-863c-1557213d69f0",
        "enabled": true,
        "checkAutomatically": "ON_LOAD",
        "fallbackToCacheTimeout": 30000
      }
    },
    "runtimeVersion": "1.0.0",
    "updates": {
      "url": "https://u.expo.dev/7a1de260-e3cb-4cbb-863c-1557213d69f0"
    }
  }
}
