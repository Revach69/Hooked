{
  "cli": {
    "version": ">= 16.17.0",
    "appVersionSource": "remote"
  },
  "build": {
    "base": {
      "node": "22.11.0",
      "cache": {
        "disabled": true
      }
    },
    "development": {
      "extends": "base",
      "developmentClient": true,
      "env": {
        "EXPO_PUBLIC_ENV": "development",
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON_DEV",
        "GOOGLE_SERVICE_INFO_PLIST": "$GOOGLE_SERVICE_INFO_PLIST_DEV",
        "MAPBOX_DOWNLOADS_TOKEN": "$MAPBOX_DOWNLOADS_TOKEN"
      },
      "distribution": "internal",
      "ios": {
        "resourceClass": "m-medium",
        "env": {
          "RCT_NEW_ARCH_ENABLED": "0"
        }
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "staging": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "staging",
        "EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN": "$MAPBOX_ACCESS_TOKEN",
        "MAPBOX_DOWNLOADS_TOKEN": "$MAPBOX_DOWNLOADS_TOKEN"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium",
        "buildType": "apk"
      }
    },
    "preview": {
      "extends": "base",
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "preview",
        "EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN": "$MAPBOX_ACCESS_TOKEN",
        "MAPBOX_DOWNLOADS_TOKEN": "$MAPBOX_DOWNLOADS_TOKEN"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "mapbox-dev": {
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_ENV": "development",
        "EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN": "$MAPBOX_ACCESS_TOKEN_DEV",
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON_DEV",
        "GOOGLE_SERVICE_INFO_PLIST": "$GOOGLE_SERVICE_INFO_PLIST_DEV",
        "MAPBOX_DOWNLOADS_TOKEN": "$MAPBOX_DOWNLOADS_TOKEN"
      },
      "ios": {
        "resourceClass": "m-medium"
      },
      "android": {
        "resourceClass": "medium"
      }
    },
    "production": {
      "extends": "base",
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN": "$MAPBOX_ACCESS_TOKEN",
        "GOOGLE_SERVICES_JSON": "$GOOGLE_SERVICES_JSON",
        "GOOGLE_SERVICE_INFO_PLIST": "$GOOGLE_SERVICE_INFO_PLIST",
        "MAPBOX_DOWNLOADS_TOKEN": "$MAPBOX_DOWNLOADS_TOKEN"
      },
      "ios": {
        "resourceClass": "m-medium",
        "env": {
          "RCT_NEW_ARCH_ENABLED": "0"
        }
      },
      "android": {
        "resourceClass": "medium",
        "env": {
          "EXPO_USE_DEV_SERVER": "false"
        },
        "buildType": "app-bundle"
      }
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6748921014"
      }
    }
  }
}
