{
  "expo": {
    "name": "Hooked Dev",
    "slug": "hooked-dev",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/icon-rounded.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.hookedapp.app.dev",
      "splash": {
        "image": "./assets/icon-rounded.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/icon-rounded.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      },
      "associatedDomains": [
        "applinks:dev.hooked-app.com",
        "applinks:dev-www.hooked-app.com"
      ],
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false,
        "NSCameraUsageDescription": "Hooked Dev needs camera access to let you take profile photos and scan QR codes at events.",
        "NSPhotoLibraryUsageDescription": "Hooked Dev needs photo library access to let you select existing photos for your dating profile at events.",
        "NSUserNotificationUsageDescription": "Hooked Dev needs notification access to send you alerts about new matches and messages at events.",
        "UIBackgroundModes": ["remote-notification"]
      },
      "entitlements": {
        "aps-environment": "development"
      },
      "googleServicesFile": "./ios/Hooked/GoogleService-Info-dev.plist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/icon.png",
        "backgroundColor": "#ffffff"
      },
      "splash": {
        "image": "./assets/icon-rounded.png",
        "resizeMode": "contain",
        "backgroundColor": "#ffffff",
        "dark": {
          "image": "./assets/icon-rounded.png",
          "resizeMode": "contain",
          "backgroundColor": "#1a1a1a"
        }
      },
      "package": "com.hooked.hookeddev",
      "googleServicesFile": "./android/app/google-services-dev.json",
      "intentFilters": [
        {
          "action": "VIEW",
          "autoVerify": true,
          "category": ["BROWSABLE", "DEFAULT"],
          "data": [
            {
              "scheme": "https",
              "host": "dev.hooked-app.com"
            },
            {
              "scheme": "https", 
              "host": "dev-www.hooked-app.com"
            }
          ]
        }
      ],
      "permissions": [
        "android.permission.INTERNET",
        "android.permission.CAMERA",
        "android.permission.READ_MEDIA_IMAGES",
        "android.permission.POST_NOTIFICATIONS",
        "android.permission.VIBRATE"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      [
        "@sentry/react-native/expo",
        {
          "organization": "hooked",
          "project": "hooked-dev",
          "url": "https://sentry.io/"
        }
      ],
      "expo-router",
      [
        "expo-notifications",
        {
          "icon": "./assets/icon.png",
          "color": "#ffffff",
          "androidMode": "default",
          "androidCollapsedTitle": "New Message [DEV]",
          "androidChannelId": "default-dev",
          "androidChannelName": "Hooked Dev Notifications",
          "androidChannelDescription": "Development notifications for new messages and matches",
          "androidChannelImportance": "high",
          "androidShowBadge": true,
          "androidVibrate": true,
          "androidSound": "default"
        }
      ],
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static",
            "useModularHeaders": true,
            "newArchEnabled": false
          }
        }
      ]
    ],
    "scheme": "hooked-dev", 
    "extra": {
      "firebaseConfig": {
        "apiKey": "AIzaSyBhGESZT7wVnFX7zLm6_ztJJHzKL5JLhf8",
        "authDomain": "hooked-development.firebaseapp.com",
        "projectId": "hooked-development",
        "storageBucket": "hooked-development.firebasestorage.app",
        "messagingSenderId": "474843121153",
        "appId": "1:474843121153:web:7242ff23e23f158383a200",
      },
      "router": {},
      "eas": {
        "projectId": "7a1de260-e3cb-4cbb-863c-1557213d69f0"
      },
      "updates": {
        "url": "https://u.expo.dev/7a1de260-e3cb-4cbb-863c-1557213d69f0"
      }
    }
  }
}
